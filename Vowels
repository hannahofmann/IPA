<div id="animationContainer"> </div>
<p><script src="https://cdnjs.cloudflare.com/ajax/libs/bodymovin/5.5.9/lottie.min.js"></script> <script>
    document.querySelectorAll(".letter").forEach(letter => {
        letter.addEventListener("click", function() {
            const animationPath = this.getAttribute("data-animation");
            const soundPath = this.getAttribute("data-sound");

            const animationContainer = document.getElementById("animationContainer");
            animationContainer.innerHTML = ""; // Clear previous animation

            const animation = lottie.loadAnimation({
                container: animationContainer,
                renderer: "svg",
                loop: false,
                autoplay: true,
                path: animationPath
            });

            const audio = new Audio(soundPath);
            audio.play();

           
        });
    });
</script></p>
